{

  "scripts": {

    "test": "cypress run --spec cypress/e2e/1-getting-started/*.js --headless",

    "test1": "cypress run --spec cypress/e2e/2-advanced-examples/*.js --headed",

    "test2": "cypress run --spec cypress/e2e/2-advanced-examples/api_test.cy.js --headed",

    "test3": "cypress run --spec cypress/e2e/2-advanced-examples/api_test.cy.js --headless",

    "test4": "cypress run --spec cypress/e2e/2-advanced-examples/web_test.cy.js --headed",

    "test5": "cypress run --spec cypress/e2e/2-advanced-examples/swag_labs.cy.js --headed --browser chrome",

    "test6": "npx cypress run --headed",

    "test7": "cypress run --spec cypress/e2e/2-advanced-examples/email_test.cy.js --headed",

    "cy:parallel": "cypress-parallel -d cypress/e2e/ -t 4 -s test6",

